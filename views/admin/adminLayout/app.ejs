    $("#addProductBtn").click(()=>{
      let 
        name=$("#name").val() , 
        price=$("#price").val() , 
        brand=$("#brand").val() , 
        colors=$("#colors").val() , 
        sizes =$("#sizes").val(), 
        stock=$("#stock").val() , 
        description=$("#description").val() ;
      let file = $("#file").prop('files');
      let details = {
          name,
          price,
          brand,
          colors,
          sizes,
          stock,
          description,
        };

      let fdata = new FormData();
      fdata.append('image'  , file[0]);
      fdata.append('details' , JSON.stringify(details));
      addProduct(fdata);

    })
